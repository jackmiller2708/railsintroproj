<h1>Listing all Games</h1>

<%=paginate  @game %>

<table class="table">
  <thead class="thead-dark">
    <tr>
      <th>Title</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <%  @game.each do |game| %>
      <tr>
        <td><%= game.name %></td>
        <td><%= link_to 'Show', game_path(game) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

